<Page x:Class="HealthCare.View.Patient.QuestionAnswer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HealthCare"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="1280"
      Title="QuestionAnswer">
    
    <Page.Resources>
        <ResourceDictionary Source="/View/Patient/ThemeResourceDictionary.xaml"/>
    </Page.Resources>
    
    <StackPanel Margin="200,50,200,50">
        <TextBlock Text="{Binding }" TextWrapping="Wrap" FontSize="20"/>
        <TextBlock Text="{Binding }" TextWrapping="Wrap" FontSize="20" Margin="0,30,0,0"/>
    </StackPanel>
</Page>
