<Window x:Class="HealthCareClinic.View.Notifications"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HealthCareClinic.View"
        xmlns:model="clr-namespace:HealthCareClinic.View.Model"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        MaxHeight="500"
        Title="HealthCare Clinic" 
        WindowStartupLocation = "CenterScreen"
        Icon="../../View/Doctor/ViewResources/Images/hospitalicon.ico"
        ResizeMode="NoResize">

    <Grid Focusable="False">
        <model:NotificationDataView Focusable="False" x:Name="DataViewer" Grid.Column="0" />
    </Grid>
</Window>
